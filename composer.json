{
    "name": "juniyasyos/laravel-iam-client",
    "description": "Laravel package for IAM SSO integration with JIT user provisioning",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "sso",
        "iam",
        "jwt",
        "authentication"
    ],
    "authors": [
        {
            "name": "Juniyasyos",
            "email": "juniyasyos@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "firebase/php-jwt": "^6.0",
        "spatie/laravel-permission": "^5.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Juniyasyos\\IamClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juniyasyos\\IamClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juniyasyos\\IamClient\\IamClientServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}